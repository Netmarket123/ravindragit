{
    "extends": "airbnb",
    "env": {
        "mocha": true
    },
    "plugins": [
        "react-native"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        },
    },
    "rules": {
        "no-empty-label": 0,
        "space-before-keywords": 0,
        "space-after-keywords": 0,
        "space-return-throw-case": 0,
        "react-native/no-unused-styles": 2,
        "react-native/split-platform-components": 2
    }
}
